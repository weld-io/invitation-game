p, h1, h2, h3 {
	color: #4e5050;
}

p {
	font-size: 1.3em;
}

a {
	color: #6ec8dc;
	text-decoration: none;
}

.call-to-action {
	border: none;
	background-color: #47e2a1;
	color: white;
	cursor: pointer;
	text-align: center;
	text-decoration: none;
	height: 60px;
	width: 200px;
	font-size: 1.5em;
	border-radius: 3px;
	padding: 0.5em;
}
